	<!-- ****************************************************************************** PhraseCatalogue *****  -->
	<xs:complexType name="PhraseCatalogue">
		<xs:annotation>
			<xs:documentation>
				Catalogue of standard phrases used in the datasheet. Used in chapter 0 and referred in for example exposure scenarios. 
				PhraseCatalogues and PhraseIds simplifies translation to other languages. Example: EUPhraC catalogue.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PhraseCatalogueName" type="string256" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Phrase catalogue Name, eg. EuPhraC</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PhraseCatalogueIssuer" type="string256" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Phrase catalogue issuer, eg. EuPhraC-Workinggroup </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PhraseCatalogueVersion" type="string64" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The version no of the phrase catalog the information is based on</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PhraseCatalogueDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>The version timestamp of the phrase catalogue the information is based on</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="PhraseCatalogueId" type="xs:integer" default="0"/>
	</xs:complexType>
	<!-- ******************************************************************** PhraseId ********  -->
	<xs:simpleType name="PhraseId">
		<xs:annotation>
			<xs:documentation>A phrase identifier which is unique within a given phrase catalogue, represented by a string of maximum 128 characters.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="128"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ******************************************************************************* Phrase ******************  -->
	<xs:complexType name="Phrase">
		<xs:annotation>
			<xs:documentation>
				Phrase, using optional references to a coordination body assigning ids to all phrases.
				PhraseId is a unique identifier (primary key) that must exist in each phrase catalogue. PhraseId has precedence over textual phrase values if both are present.
				PhraseCode is any official or system-internal phrase identifier which may be unique or not (and is often empty). 
				Example: In EuPhraC, PhraseIds 9278174452 to 9278174455 all refer to PhraseCode = H341, however EuPhraC decided to name the other codes as H341.1, H341.2 and H341.3.
				Note the use of MergePhrase restricts the nesting depth to one level.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PhraseCode" type="string64" minOccurs="0"/>
			<xs:element name="FullText" type="string1024" minOccurs="0" maxOccurs="1"/>
			<xs:element name="MergePhrase" type="MergePhrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="PhraseId" type="PhraseId"/>
		<xs:attribute name="PhraseCatalogueId" type="xs:integer" default="0"/>
		<!-- Phase catalogues used are defined and assigned a PhraseCatalogueId in section 0 -->
	</xs:complexType>
	<!-- ************************************************************* MergePhrase ********************  -->
	<xs:complexType name="MergePhrase">
		<xs:annotation>
			<xs:documentation>
				Phrase, using references to a coordination body assigning ids to all phrases. Ids have precedence over textual phrase values if both are present.
				Note the use of MergePhrase restricts the nesting depth to one level.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MergeText" type="string512" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Delimiter" type="string3" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
		<xs:attribute name="PhraseId" type="PhraseId"/>
		<xs:attribute name="PhraseCatalogueId" type="xs:integer" default="0"/>
		<xs:attribute name="MergePhraseNo" type="xs:integer" default="0"/>
		<xs:attribute name="ListItemNo" type="xs:integer" default="0"/>
	</xs:complexType>
	<!-- ************************************************************* RiskPhrase ***************************  -->
	<xs:complexType name="RiskPhrase">
		<xs:annotation>
			<xs:documentation>
				Risk Phrases DPD, DSD
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PhraseCode" type="RiskPhraseEnum" minOccurs="0"/>
			<xs:element name="FullText" type="string512" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
		<xs:attribute name="PhraseId" type="PhraseId"/>
		<xs:attribute name="PhraseCatalogueId" type="xs:integer" default="0"/>
	</xs:complexType>
	<!-- ************************************************************* SafetyPhrase ***************************  -->
	<xs:complexType name="SafetyPhrase">
		<xs:annotation>
			<xs:documentation>
				Safety Phrases DPD, DSD
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PhraseCode" type="SafetyPhraseEnum" minOccurs="0"/>
			<xs:element name="FullText" type="string512" minOccurs="0" maxOccurs="1"/>
			<xs:element name="MergePhrase" type="MergePhrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="PhraseId" type="PhraseId"/>
		<xs:attribute name="PhraseCatalogueId" type="xs:integer" default="0"/>
	</xs:complexType>
	<!-- ************************************************************* HazardPictogram ***************************  -->
	<xs:complexType name="HazardPictogram">
		<xs:annotation>
			<xs:documentation>
				CLP Hazard pictogram
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PhraseCode" type="PictogramCodeEnum" minOccurs="1" maxOccurs="1"/>
			<xs:element name="FullText" type="string512" minOccurs="0" maxOccurs="1"/>
			<xs:element name="MergePhrase" type="MergePhrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="PhraseId" type="PhraseId"/>
		<xs:attribute name="PhraseCatalogueId" type="xs:integer" default="0"/>
	</xs:complexType>
	<!-- ************************************************************* SignalWord ***************************  -->
	<xs:complexType name="SignalWord">
		<xs:annotation>
			<xs:documentation>
				CLP Signal Word, ie. danger or warning.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PhraseCode" type="SignalWordEnum" minOccurs="1" maxOccurs="1"/>
			<xs:element name="FullText" type="string512" minOccurs="0" maxOccurs="1"/>
			<xs:element name="MergePhrase" type="MergePhrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="PhraseId" type="PhraseId"/>
		<xs:attribute name="PhraseCatalogueId" type="xs:integer" default="0"/>
	</xs:complexType>
	<!-- ************************************************************* HazardStatement ***************************  -->
	<xs:complexType name="HazardStatement">
		<xs:annotation>
			<xs:documentation>CLP Hazard Statement. Systems can export PhraseCode only, without FullText or other phrase references - this is useful if data tranfer of only SDS parts takes place without any use of phrases.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PhraseCode" type="HazardStatementEnum" minOccurs="1" maxOccurs="1"/>
			<xs:element name="FullText" type="string512" minOccurs="0" maxOccurs="1"/>
			<xs:element name="MergePhrase" type="MergePhrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="PhraseId" type="PhraseId"/>
		<xs:attribute name="PhraseCatalogueId" type="xs:integer" default="0"/>
	</xs:complexType>
	<!-- ************************************************************* PrecautionaryStatement ***************************  -->
