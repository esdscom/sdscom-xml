	<xs:simpleType name="RmmPictogramEnum">
		<xs:annotation>
			<xs:documentation>Mandatory signs related to Risk Management Measures, according to EN ISO 7010</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="M0[0-2][0-9]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AgpaPictogramEnum">
		<xs:annotation>
			<xs:documentation>P0xx: Prohibition signs related to Additional Good Practice Advice according to EN ISO 7010, and/or AISExx: A.I.S.E. CleanRight Safe Use Icons Version 4, numbered according to the sentences on pp. 16-18 of https://www.aise.eu/documents/document/20140129161815-final_draft_aise_safe_use_guidelines_revjan2014.pdf</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="P0[0-3][0-9]|AISE[0-1][0-9]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="EquipmentPictogramEnum">
		<xs:annotation>
			<xs:documentation>Equipment signs related to Technical Measures, according to EN ISO 7010</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="E0[0-1][0-9]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FileName">
		<xs:annotation>
			<xs:documentation>Name of a separate physical file. For a proper data exchange, the file name should contain no path information or a relative path which applies during information exchange, e.g. within a ZIP file containing SDScom XML and related documents.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="eSDScom:string256"/>
	</xs:simpleType>
	<xs:simpleType name="CasNo">
		<xs:annotation>
			<xs:documentation> The CAS identification string for a substance. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{2,7}-\d{2}-\d{1}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DunsNo">
		<xs:annotation>
			<xs:documentation>The D-U-N-S® Number of a legal entity. See https://www.upik.de/</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{9}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RoleDescriptionEnum">
