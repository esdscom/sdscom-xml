	<!-- ********************************************************************** float8 ***********  -->
	<xs:simpleType name="float8">
		<xs:annotation>
			<xs:documentation>
				A float value with precision, that is pH 7.0 differs from pH 7.00 which is a more precise 
				specification. Exponential values like "-1.235E5" are allowed. Max 8 characters allowed. 
				Strings with no digits at all should not be allowed.
		</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="8"/>
			<xs:pattern value="[-+]?[0-9]*\.?[0-9]+([eE][-+]?[0-9]+)?"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ********************************************************************** float11 ***********  -->
	<xs:simpleType name="float11">
		<xs:annotation>
			<xs:documentation>
				A float value with precision, that is pH 7.0 differs from pH 7.00 which is	a more precise
				specification. Exponential values like "-1.235E5" are allowed. Max 11 characters allowed.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="11"/>
			<xs:pattern value="[-+]?[0-9]*\.?[0-9]+([eE][-+]?[0-9]+)?"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ********************************************************************** float16 ***********  -->
	<xs:simpleType name="float16">
		<xs:annotation>
			<xs:documentation>
				A float value with precision, that is pH 7.0 differs from pH 7.00 which is	a more precise
