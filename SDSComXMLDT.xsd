	</xs:simpleType>
	<!-- *******************************************************************  GasGroup *********************************  -->
	<xs:complexType name="GasGroup">
		<xs:annotation>
			<xs:documentation>
				GasGroup. Source: Chapter 9.		
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Method" type="Phrase" minOccurs="0"/>
			<xs:element name="Source" type="Phrase" minOccurs="0"/>
			<xs:element name="TestResults" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Evaluation" type="GasGroupEnum" minOccurs="0"/>
			<xs:element name="Comments" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- *******************************************************************  BurningNumber ***********  -->
	<!-- *******************************************************************  Species *************  -->
	<xs:complexType name="Species">
		<xs:annotation>
			<xs:documentation>
				Species used in tests
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Value" type="Phrase"/>
			<xs:element name="Gender" type="Phrase" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ********************************************************************  WasteCode ***************  -->
	<xs:simpleType name="WasteCode">
		<xs:annotation>
			<xs:documentation>
				The European waste code, eg, "20 01 01".
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{2} [0-9]{2} [0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ******************************************************************* AssessmentEnum *************  -->
	<xs:simpleType name="AssessmentEnum">
		<xs:annotation>
			<xs:documentation>
				Legal values for the AssessmentEnum 
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Yes"/>
			<xs:enumeration value="No"/>
			<xs:enumeration value="Not assessed"/>
		</xs:restriction>
	</xs:simpleType>
