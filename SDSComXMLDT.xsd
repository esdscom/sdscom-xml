	<!-- ******************************************************************** CasNo *****  -->
	<xs:simpleType name="CasNo">
		<xs:annotation>
			<xs:documentation>
				The CAS identification string for a substance.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{2,7}-\d{2}-\d{1}"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ******************************************************************** EcNo *****  -->
	<xs:simpleType name="DunsNo">
