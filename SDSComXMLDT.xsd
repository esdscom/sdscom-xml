	<!-- ******************************************************************* UnitValue *************  -->
	<xs:complexType name="UnitValue">
		<xs:annotation>
			<xs:documentation>
				UnitValue. The ExactValue should only be printed on the datasheet if both the LowerValue and UpperValue
				are missing. The main purpose of the LowerValue-UpperValue range is for printing while an additional 
				ExactValue can be used for calculations.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Value">
				<xs:sequence>
					<xs:element name="Unit" type="string32" minOccurs="0"/>
				</xs:sequence>
				<xs:attribute name="UnitId" type="UnitIdEnum"/>
				<xs:attribute name="UnitSetId" type="xs:integer">
					<xs:annotation>
						<xs:documentation>This is the Norwegian unit Id. Please see http://pib.no/Main.aspx?Gui/UnitSet#Top</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
