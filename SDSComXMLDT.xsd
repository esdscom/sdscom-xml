	<!-- ******************************************************************** Percentage********  -->
	<xs:simpleType name="Percentage">
		<xs:annotation>
			<xs:documentation>Percentage value 0-100.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:float">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ******************************************************************* ReasonForWaiving *************  -->
	<xs:simpleType name="ReasonForWaiving">
		<xs:annotation>
			<xs:documentation>For physical or chemical properties, reasons for waiving are:
1. cannot be determined = technically impossible to obtain (pH value for solids, arguably)
2. not applicable = irrelevant (because no hazard assessment requires such info)
3. no data = unknown (not tested so far)
Implemented for https://github.com/esdscom/sdscom-xml/issues/33</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="cannot be determined"/>
			<xs:enumeration value="not applicable"/>
			<xs:enumeration value="no data"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ******************************************************************* PercentageValue *************  -->
	<xs:complexType name="PercentageValue">
		<xs:annotation>
			<xs:documentation>Basically a UnitValue with a fixed unit "%"</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Value"/>
		</xs:complexContent>
	</xs:complexType>
	<!-- ******************************************************************* PhysChemValue***********  -->
	<xs:complexType name="PhysChemValue">
		<xs:annotation>
			<xs:documentation>
				PhysChemValue, used to specify a value derived from a duration-independent measurement. 
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:sequence>
					<xs:element name="Value" type="Value" minOccurs="0"/>
					<xs:element name="Method" type="Phrase" minOccurs="0"/>
					<xs:element name="TestReference" type="Phrase" minOccurs="0"/>
				</xs:sequence>
				<xs:element minOccurs="0" name="ReasonForWaiving" type="ReasonForWaiving"/>
			</xs:choice>
			<xs:element name="Comments" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Temperature" type="UnitValue" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ******************************************************************* PhysChemUnitValue *************************  -->
	<xs:complexType name="PhysChemUnitValue">
		<xs:annotation>
			<xs:documentation>
				PhysChemUnitValue, used to specify a value with unit derived from a duration-independent measurement. 
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:sequence>
					<xs:element name="UnitValue" type="UnitValue" minOccurs="0"/>
					<xs:element name="Method" type="Phrase" minOccurs="0"/>
					<xs:element name="TestReference" type="Phrase" minOccurs="0"/>
				</xs:sequence>
				<xs:element minOccurs="0" name="ReasonForWaiving" type="ReasonForWaiving"/>
			</xs:choice>
			<xs:element name="Comments" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Temperature" type="UnitValue" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ******************************************************************* PhysChemValueWithTemperature **************  -->
	<xs:complexType name="PhysChemValueWithTemperature">
		<xs:annotation>
			<xs:documentation>
				PhysChemValueWithTemperature. Source: Chapter 9.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PhysChemValue"> </xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ******************************************************************* PhysChemUnitValueWithTemperature ***********  -->
	<xs:complexType name="PhysChemUnitValueWithTemperature">
		<xs:annotation>
			<xs:documentation>
				PhysChemUnitValueWithTemperature. Source: Chapter 9.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PhysChemUnitValue"> </xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ******************************************************************* TemperatureClassEnum ************************  -->
	<xs:simpleType name="TemperatureClassEnum">
		<xs:annotation>
			<xs:documentation>
				Legal values for the TemperatureClassEnum. Source: Chapter 9.				
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="T 1"/>
			<xs:enumeration value="T 2"/>
			<xs:enumeration value="T 3"/>
			<xs:enumeration value="T 4"/>
			<xs:enumeration value="T 5"/>
			<xs:enumeration value="T 6"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- *******************************************************************  TemperatureClass **************************  -->
	<xs:complexType name="TemperatureClass">
		<xs:annotation>
			<xs:documentation>
				TemperatureClass 
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Method" type="Phrase" minOccurs="0"/>
			<xs:element name="Source" type="Phrase" minOccurs="0"/>
			<xs:element name="TestResults" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Evaluation" type="TemperatureClassEnum" minOccurs="0"/>
			<xs:element name="Comments" type="Phrase" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- *****************************************************************  PhysicalHazardsGeneralInformation ***********  -->
	<xs:complexType name="PhysicalHazardsGeneralInformation">
		<xs:annotation>
			<xs:documentation>
				PhysicalHazardsGeneralInformation. Source: Chapter 9.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="JustificationForDataWaiving" type="Phrase" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Assessment" type="Phrase" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Classification" type="Phrase" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ******************************************************************* GasGroupEnum ******************************  -->
	<xs:simpleType name="GasGroupEnum">
		<xs:annotation>
			<xs:documentation>
				Legal values for the GasGroupEnum. Source: Chapter 9.			
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="IIA"/>
