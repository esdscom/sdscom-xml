	<xs:complexType name="NumericValue">
		<xs:sequence>
			<xs:element name="ExactValue" type="xs:float"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NumericValueWithQualifier">
		<xs:sequence>
			<xs:element name="ExactValueSymbol" type="eSDScom:ExactQualifierEnum" minOccurs="0"/>
			<xs:element name="ExactValue" type="xs:float"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NumericValueWithUnit">
		<xs:complexContent>
			<xs:extension base="eSDScom:NumericValue">
				<xs:group ref="eSDScom:Unit"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="NumericValueWithUnitAndQualifier">
		<xs:complexContent>
			<xs:extension base="eSDScom:NumericValueWithQualifier">
