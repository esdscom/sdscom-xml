												<xs:annotation>
													<xs:appinfo>SDScomBau</xs:appinfo>
													<xs:appinfo>SDScomChem</xs:appinfo>
													<xs:documentation> Process category: PROC </xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element maxOccurs="unbounded" minOccurs="0" name="EnvironmentalReleaseCategory" type="eSDScom:EnvironmentalReleaseCategory">
