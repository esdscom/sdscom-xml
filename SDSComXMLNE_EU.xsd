			<xs:element name="Assessment" type="eSDScom:AssessmentEnum"/>
			<xs:element minOccurs="0" name="AssessmentFulltext" type="eSDScom:Phrase"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ComponentEU">
		<xs:complexContent>
