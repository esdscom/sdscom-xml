                        <xs:element name="HealthChronic" type="boolean"/>
                        <xs:element name="Flammability">
                            <xs:simpleType>
                                <xs:restriction base="int">
                                    <xs:minInclusive value="0"/>
                                    <xs:maxInclusive value="4"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="Instability">
