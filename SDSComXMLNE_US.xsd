<xs:schema targetNamespace="http://www.esdscom.eu/SDScom" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:SDScom="http://www.esdscom.eu/SDScom" xmlns:xs="http://www.w3.org/2001/XMLSchema" version="5.0.0">
    <xs:include schemaLocation="SDSComXMLDT.xsd"/>
    <xs:include schemaLocation="SDSComXMLCT.xsd"/>
    <xs:simpleType name="Hazcom2012ClassCategoryEnumRestr">
        <xs:annotation>
            <xs:documentation>This type restricts GHS revision 3 to the classes and categories which are covered in HazCom 2012. Since environmental hazards are optional, they are not excluded here.</xs:documentation>
        </xs:annotation>
        <xs:restriction>
            <xs:simpleType>
                <xs:restriction base="SDScom:Ghs03ClassCategoryEnum">
                    <xs:enumeration value="Acute Tox. 5 (oral)"/>
                    <xs:enumeration value="Acute Tox. 5 (dermal)"/>
                    <xs:enumeration value="Acute Tox. 5 (inhalation)"/>
                    <xs:enumeration value="Skin Irrit. 3"/>
                    <xs:enumeration value="Asp. Tox. 2"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Hazcom2012ClassCategoryEnumExt">
        <xs:annotation>
            <xs:documentation>U.S. specific hazard classes</xs:documentation>
        </xs:annotation>
        <xs:restriction base="string">
            <xs:enumeration value="Pyrophoric Gas"/>
            <xs:enumeration value="Simple Asphyxiant"/>
            <xs:enumeration value="Combustible Dust"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Hazcom2012ClassCategoryEnum">
        <xs:union memberTypes="SDScom:Hazcom2012ClassCategoryEnumRestr SDScom:Hazcom2012ClassCategoryEnumExt"/>
    </xs:simpleType>
    <xs:simpleType name="UnIdNo">
        <xs:restriction base="string">
            <xs:pattern value="(UN|NA)\d\d\d\d"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="ClassificationUS">
        <xs:sequence>
            <xs:element name="Hazcom2012HazardClassification" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Classification according to 29 CFR 1910.1200</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:sequence maxOccurs="unbounded" minOccurs="0">
                            <xs:element maxOccurs="1" minOccurs="1" name="HazardClassCategory" type="SDScom:Hazcom2012ClassCategoryEnum">
                                <xs:annotation>
                                    <xs:documentation>The hazard class and a corresponding category / differentiation according to Hazcom 2012. </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="ClassificationProcedure" type="SDScom:Phrase">
                                <xs:annotation>
                                    <xs:documentation>To state how the individual classification is prepared. (This information should be shown in section 16). Only relevant for instances of type Classification in section 2. For components in section 3, this info should not be part of the SDS.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="AdditionalInformation" type="SDScom:Phrase">
                            <xs:annotation>
                                <xs:appinfo>SDScomBau</xs:appinfo>
                                <xs:appinfo>SDScomChem</xs:appinfo>
                                <xs:documentation> Additional information on classification: If the chemical do not meet the classification criteria etc.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="HazardLabellingUS">
        <xs:sequence>
            <xs:element minOccurs="0" name="Hazcom2012HazardLabelling">
                <xs:annotation>
                    <xs:appinfo>SDScomBau</xs:appinfo>
                    <xs:appinfo>SDScomChem</xs:appinfo>
                    <xs:documentation>Labelling according to EU regulation 1272/2008, revised until and including EU regulation 2016/918, based of GHS revision 5.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="6" minOccurs="0" name="HazardPictogram" type="SDScom:GhsSignalWordEnum">
                            <xs:annotation>
                                <xs:appinfo>SDScomBau</xs:appinfo>
                                <xs:appinfo>SDScomChem</xs:appinfo>
                                <xs:documentation> Pictogram: A graphical presentation that includes a symbol plus other graphic elements, such as a border, background pattern or colour that is intended to convey specific information on the hazard concerned. </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="SignalWord" type="SDScom:GhsSignalWord">
                            <xs:annotation>
                                <xs:appinfo>SDScomBau</xs:appinfo>
                                <xs:appinfo>SDScomChem</xs:appinfo>
                                <xs:documentation> Signal word: A word that indicates the relative level of severity of hazards to alert the reader to a potential hazard. </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="HazardStatement" type="SDScom:Ghs03HazardStatement">
                            <xs:annotation>
                                <xs:appinfo>SDScomBau</xs:appinfo>
                                <xs:appinfo>SDScomChem</xs:appinfo>
                                <xs:documentation> Hazard statement: A phrase assigned to a hazard class and category that describes the nature of the hazards of a hazardous substance or mixture, including, where appropriate, the degree of hazard. </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="PrecautionaryStatement" type="SDScom:Ghs03PrecautionaryStatement">
                            <xs:annotation>
                                <xs:appinfo>SDScomBau</xs:appinfo>
                                <xs:appinfo>SDScomChem</xs:appinfo>
                                <xs:documentation> Precautionary statement: A phrase that describes recommended measure(s) to minimise or prevent adverse effects resulting from exposure to a hazardous substance or mixture due to its use or disposal. </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="AdditionalLabellingInfo" type="SDScom:Phrase">
                            <xs:annotation>
                                <xs:appinfo>SDScomChem</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="TactileWarning" type="boolean">
                            <xs:annotation>
                                <xs:documentation> Tactile Warning: To inform if packaging must be supplied with tactile warning if the chemical is supplied to the gerneral public </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="ChildResistantOpening" type="boolean">
                            <xs:annotation>
                                <xs:documentation> Child Resistant Fastening: To inform if the packaging must be supplied with such fastening if the chemical is supplied to the general public. </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="LabellingAccordingToOtherLegislation">
                <xs:annotation>
                    <xs:appinfo>SDScomChem</xs:appinfo>
                    <xs:documentation> Other elements of the hazard label in accordance with EU legislation: Labelling required by other EU legislation </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="VocLabelling">
                            <xs:annotation>
                                <xs:appinfo>SDScomChem</xs:appinfo>
                                <xs:documentation>VOC Labelling: Labelling requirements according to directive 2004/42/EC (amending 1999/13/EC)</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element minOccurs="0" name="ProductSubcategory" type="SDScom:Phrase">
                                        <xs:annotation>
                                            <xs:appinfo>SDScomChem</xs:appinfo>
                                            <xs:documentation> Product Subcategory: The type of paint or varnish </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element minOccurs="0" name="VocLimitForSubcategory" type="SDScom:UnitValue">
                                        <xs:annotation>
                                            <xs:appinfo>SDScomChem</xs:appinfo>
                                            <xs:documentation> Maximum VOC content limit for the mixture: The maximum concentration of VOC in the actual product. To begiven i gram per litre. </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element minOccurs="0" name="MaxVocConcInMixture" type="SDScom:UnitValue">
                                        <xs:annotation>
                                            <xs:appinfo>SDScomChem</xs:appinfo>
                                            <xs:documentation> Actual max. concentration in the mixture: The concentration of VOC in the mixture. To begiven i gram per litre. </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="DetergentLabelling" type="SDScom:Phrase">
                            <xs:annotation>
                                <xs:documentation> Detergent labelling: Labelling requirements according to detergents regulation (648/2004/EC with later amanedments). Includes dosing recommendations. Concentration groups: _lt 5 weight%, 5 - _lt 15 vekt%, 15 - _lt 30 weight%, og _ge 30 weight%. </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="AerosolLabelling" type="SDScom:Phrase">
                            <xs:annotation>
                                <xs:documentation> Aerosol labelling: Labelling of aerosol dispensers in accordance with EU directive (75/324/EEC with later amendments) </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="OtherEuLabellingRequirements" type="SDScom:Phrase">
                            <xs:annotation>
                                <xs:documentation> Other labelling requirenments in accordance with EU legislation: Labelling requirements imposed to the chemical in accordance with EU legislation not stated above. </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ComponentUS">
        <xs:complexContent>
            <xs:extension base="SDScom:Component">
                <xs:sequence>
                    <xs:element minOccurs="0" name="RegionUS">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element minOccurs="0" name="Classification" type="SDScom:ClassificationUS"> </xs:element>
                                <xs:element maxOccurs="1" minOccurs="0" name="OnLabel" type="boolean">
                                    <xs:annotation>
                                        <xs:appinfo>SDScomChem</xs:appinfo>
                                        <xs:documentation> Name of the component to be displayed on the hazard label: The value "True" for this means that the name of the actual component shall be printed on the hazard label as well as in section 2 of the SDS under the sub-heading 'Dangerous substances'�. The default value of this element will be 'false'�. </xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element minOccurs="0" name="TransportInformation">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element maxOccurs="1" minOccurs="0" name="IsDangerReleasingSubstance" type="boolean">
                                                <xs:annotation>
                                                    <xs:documentation>according to DOT</xs:documentation>
                                                </xs:annotation>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                                <xs:element minOccurs="0" name="RegulatoryInformation">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element minOccurs="0" name="Federal">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element minOccurs="0" name="Sara313Threshold" type="SDScom:Value"/>
                                                        <xs:element minOccurs="0" name="Sara302HazardousRQ" type="SDScom:Value"/>
                                                        <xs:element minOccurs="0" name="Sara302ExtremelyHazardousRQ" type="SDScom:Value"/>
                                                        <xs:element minOccurs="0" name="Sara302ExtremelyHazardousTPQ" type="SDScom:Value"/>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element minOccurs="0" name="State">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element minOccurs="0" name="California">
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element minOccurs="0" name="Proposition65">
                                                                        <xs:complexType>
                                                                            <xs:choice>
                                                                                <xs:element name="Candidate" type="boolean">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>Allows to indicate substances under discussion for inclusion</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:element>
                                                                                <xs:sequence>
                                                                                    <xs:element name="Listed" type="boolean"/>
                                                                                    <xs:element minOccurs="0" name="ToxType" type="string"/>
                                                                                    <xs:element minOccurs="0" name="Listing" type="string"/>
                                                                                </xs:sequence>
                                                                            </xs:choice>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                </xs:sequence>
                                                            </xs:complexType>
                                                        </xs:element>
                                                        <xs:element minOccurs="0" name="RhodeIsland">
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element minOccurs="0" name="HazardousSubstancesList">
                                                                        <xs:annotation>
                                                                            <xs:documentation>see http://www.dlt.ri.gov/occusafe/pdfs/HazardousCAS.pdf</xs:documentation>
                                                                        </xs:annotation>
                                                                        <xs:complexType>
                                                                            <xs:sequence>
                                                                                <xs:element name="Listed" type="boolean"/>
                                                                                <xs:element minOccurs="0" name="SourceAcgih" type="boolean">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>In the list, denoted by the letter T.</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:element>
                                                                                <xs:element minOccurs="0" name="SourceNfpa49" type="boolean">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>In the list, denoted by the letter F.</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:element>
                                                                                <xs:element minOccurs="0" name="SourceIarc" type="boolean">
                                                                                    <xs:annotation>
                                                                                        <xs:documentation>In the list, denoted by the letter C.</xs:documentation>
                                                                                    </xs:annotation>
                                                                                </xs:element>
                                                                            </xs:sequence>
                                                                        </xs:complexType>
                                                                    </xs:element>
                                                                </xs:sequence>
                                                            </xs:complexType>
                                                        </xs:element>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="DisposalConsiderationsUS">
        <xs:sequence>
            <xs:element minOccurs="0" name="Federal">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="RcraWasteCode" type="string"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="HazardousWasteCharacteristics" type="SDScom:Phrase"> </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="RcraBasisForListing" type="SDScom:Phrase"> </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="State">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="California">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element maxOccurs="unbounded" minOccurs="0" name="HazardousWasteCode" type="string"/>
                                    <xs:element maxOccurs="unbounded" minOccurs="0" name="HazardousWasteStatus" type="SDScom:Phrase"> </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
