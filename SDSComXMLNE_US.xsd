    <xs:complexType name="TransportUS">
        <xs:sequence>
            <xs:element minOccurs="0" name="IdNoUsaDot" type="eSDScom:UnIdNo">
                <xs:annotation>
                    <xs:documentation>U.S. transportation numbers are either preceded with "UN" or with "NA"</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="ProperShippingName" type="eSDScom:Phrase"> </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="DangerReleasingSubstance" type="eSDScom:Phrase"> </xs:element>
            <xs:element minOccurs="0" name="Class" type="eSDScom:TransportClassEnum">
                <xs:annotation>
                    <xs:documentation>The chemical’s most important transport hazard class. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="2" minOccurs="0" name="Subrisk" type="eSDScom:TransportClassEnum"/>
            <xs:element maxOccurs="3" minOccurs="0" name="HazardLabel" type="eSDScom:TransportHazardLabelUsaEnum"/>
            <xs:element minOccurs="0" name="PackingGroup" type="eSDScom:PackingGroupEnum">
                <xs:annotation>
                    <xs:appinfo>SDScomBau</xs:appinfo>
                    <xs:appinfo>SDScomChem</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="OtherInformation">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="1" minOccurs="0" name="LimitedQtyPassengerRail" type="eSDScom:string20"> </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="LimitedQtyCargo" type="eSDScom:string20"> </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="SpecialProvisions" type="eSDScom:string80"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="AdditionalInfo" type="eSDScom:Phrase"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="RegulationsUS">
        <xs:annotation>
            <xs:documentation>Regulations valid for the product of mixture as a whole. For ingredient information, refer to type ComponentUS.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="Federal">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="EpaTriChemicals">
                            <xs:annotation>
                                <xs:documentation>Toxics Release Inventory (TRI) Program, https://www.epa.gov/toxics-release-inventory-tri-program</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="listed" type="xs:boolean"/>
                                    <xs:element minOccurs="0" name="CategoryName" type="eSDScom:string10"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element minOccurs="0" name="EpaPesticideRegNo" type="string"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="EpaPesticideStatement" type="eSDScom:Phrase"> </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="EpaPesticideLabel" type="eSDScom:Phrase"> </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="State">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="California">
