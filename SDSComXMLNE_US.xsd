<xs:schema targetNamespace="http://www.esdscom.eu/eSDScom" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:eSDScom="http://www.esdscom.eu/eSDScom" xmlns:xs="http://www.w3.org/2001/XMLSchema" version="5.4.0">
    <xs:include schemaLocation="SDSComXMLDT.xsd"/>
    <xs:include schemaLocation="SDSComXMLCT.xsd"/>
    <xs:simpleType name="Hazcom2012ClassCategoryEnumGhs">
        <xs:union memberTypes="eSDScom:Ghs01ClassExpl eSDScom:Ghs00ClassFlamGas eSDScom:Ghs00ClassFlamAer eSDScom:Ghs00ClassOxGas eSDScom:Ghs00ClassPressGas eSDScom:Ghs00ClassFlamLiq eSDScom:Ghs00ClassFlamSol eSDScom:Ghs00ClassSelfReact eSDScom:Ghs00ClassPyrLiq eSDScom:Ghs00ClassPyrSol eSDScom:Ghs00ClassSelfHeat eSDScom:Ghs00ClassWaterReact eSDScom:Ghs00ClassOxLiq eSDScom:Ghs00ClassOxSol eSDScom:Ghs00ClassOrgPerox eSDScom:Ghs00ClassMetCorr eSDScom:Ghs00ClassAcuteToxOral_no5 eSDScom:Ghs00ClassAcuteToxDermal_no5 eSDScom:Ghs00ClassAcuteToxInhal_no5 eSDScom:Ghs00ClassSkinCorr_no3 eSDScom:Ghs00ClassEyeDam eSDScom:Ghs03ClassRespSens eSDScom:Ghs03ClassSkinSens eSDScom:Ghs00ClassMuta eSDScom:Ghs00ClassCarc eSDScom:Ghs00ClassRepr eSDScom:Ghs00ClassLact eSDScom:Ghs00ClassStotSe eSDScom:Ghs01ClassStotSe3 eSDScom:Ghs00ClassStotRe eSDScom:Ghs01ClassAspTox_no2 eSDScom:Ghs01ClassAquaticAcute eSDScom:Ghs01ClassAquaticChronic eSDScom:Ghs03ClassOzone"/>
    </xs:simpleType>
    <xs:simpleType name="Hazcom2012ClassCategoryEnumExt">
        <xs:annotation>
            <xs:documentation>U.S. specific hazard classes</xs:documentation>
        </xs:annotation>
        <xs:restriction base="string">
            <xs:enumeration value="Pyrophoric Gas"/>
            <xs:enumeration value="Simple Asphyxiant"/>
            <xs:enumeration value="Combustible Dust"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Hazcom2012ClassCategoryEnum">
        <xs:union memberTypes="eSDScom:Hazcom2012ClassCategoryEnumGhs eSDScom:Hazcom2012ClassCategoryEnumExt"/>
    </xs:simpleType>
    <xs:simpleType name="UnIdNo">
        <xs:restriction base="string">
            <xs:pattern value="(UN|NA)\d\d\d\d"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="ClassificationUS">
        <xs:sequence>
            <xs:element name="Hazcom2012HazardClassification" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Classification according to 29 CFR 1910.1200</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:sequence maxOccurs="unbounded" minOccurs="0">
                            <xs:element maxOccurs="1" minOccurs="1" name="HazardClassCategory" type="eSDScom:Hazcom2012ClassCategoryEnum">
                                <xs:annotation>
                                    <xs:documentation>The hazard class and a corresponding category / differentiation according to Hazcom 2012. </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" name="HazardClassCategoryPrintable" type="eSDScom:Phrase">
                                <xs:annotation>
                                    <xs:appinfo>SDScomBau</xs:appinfo>
                                    <xs:appinfo>SDScomChem</xs:appinfo>
                                    <xs:documentation>Printable, phrase based representation of HazardClassCategory. While this may be identical to HazardClassCatgory in its string representation, it provides a translated version linked to a phrase library for applications where this is useful.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="ClassificationProcedure" type="eSDScom:Phrase">
                                <xs:annotation>
                                    <xs:documentation>To state how the individual classification is prepared. (This information should be shown in section 16). Only relevant for instances of type Classification in section 2. For components in section 3, this info should not be part of the SDS.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                        <xs:group ref="eSDScom:ClassificationInfo"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Ghs03HazardStatementUS">
        <xs:annotation>
            <xs:documentation>GHS Hazard Statement. Contrary to the type Ghs03HazardStatement used in other regions, the phrase code is optional here because additional US hazard statements come without a code.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="PhraseCode" type="eSDScom:Ghs03HazardEnum">
                <xs:annotation>
                    <xs:documentation>Should be present for GHS hazard statements, and omitted only for additional US hazard statements.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="FullText" type="eSDScom:string1024"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="MergePhrase" type="eSDScom:MergePhrase"/>
        </xs:sequence>
        <xs:attribute name="PhraseId" type="eSDScom:AnyPhraseId"/>
        <xs:attribute default="0" name="PhraseCatalogueId" type="integer"/>
    </xs:complexType>
    <xs:complexType name="HazardLabellingUS">
        <xs:sequence>
            <xs:element minOccurs="0" name="Hazcom2012HazardLabelling">
                <xs:annotation>
                    <xs:appinfo>SDScomBau</xs:appinfo>
                    <xs:appinfo>SDScomChem</xs:appinfo>
                    <xs:documentation>Labelling according to EU regulation 1272/2008, revised until and including EU regulation 2016/918, based of GHS revision 5.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="6" minOccurs="0" name="HazardPictogram" type="eSDScom:GhsPictogramCodeEnum">
                            <xs:annotation>
                                <xs:appinfo>SDScomBau</xs:appinfo>
                                <xs:appinfo>SDScomChem</xs:appinfo>
                                <xs:documentation> Pictogram: A graphical presentation that includes a symbol plus other graphic elements, such as a border, background pattern or colour that is intended to convey specific information on the hazard concerned. </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="SignalWord" type="eSDScom:GhsSignalWord">
                            <xs:annotation>
                                <xs:appinfo>SDScomBau</xs:appinfo>
                                <xs:appinfo>SDScomChem</xs:appinfo>
                                <xs:documentation> Signal word: A word that indicates the relative level of severity of hazards to alert the reader to a potential hazard. </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="HazardStatement" type="eSDScom:Ghs03HazardStatementUS">
                            <xs:annotation>
                                <xs:appinfo>SDScomBau</xs:appinfo>
                                <xs:appinfo>SDScomChem</xs:appinfo>
                                <xs:documentation> Hazard statement: A phrase assigned to a hazard class and category that describes the nature of the hazards of a hazardous substance or mixture, including, where appropriate, the degree of hazard. </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="PrecautionaryStatement" type="eSDScom:Ghs03PrecautionaryStatement">
                            <xs:annotation>
                                <xs:appinfo>SDScomBau</xs:appinfo>
                                <xs:appinfo>SDScomChem</xs:appinfo>
                                <xs:documentation> Precautionary statement: A phrase that describes recommended measure(s) to minimise or prevent adverse effects resulting from exposure to a hazardous substance or mixture due to its use or disposal. </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="AdditionalLabellingInfo" type="eSDScom:Phrase">
                            <xs:annotation>
                                <xs:appinfo>SDScomChem</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="TactileWarning" type="boolean">
                            <xs:annotation>
                                <xs:documentation> Tactile Warning: To inform if packaging must be supplied with tactile warning if the chemical is supplied to the gerneral public </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="ChildResistantOpening" type="boolean">
                            <xs:annotation>
                                <xs:documentation> Child Resistant Fastening: To inform if the packaging must be supplied with such fastening if the chemical is supplied to the general public. </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="LabellingAccordingToOtherLegislation">
                <xs:annotation>
                    <xs:appinfo>SDScomChem</xs:appinfo>
                    <xs:documentation> Other elements of the hazard label in accordance with EU legislation: Labelling required by other EU legislation </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="VocLabelling">
                            <xs:annotation>
                                <xs:appinfo>SDScomChem</xs:appinfo>
                                <xs:documentation>VOC Labelling: Labelling requirements according to directive 2004/42/EC (amending 1999/13/EC)</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element minOccurs="0" name="ProductSubcategory" type="eSDScom:Phrase">
                                        <xs:annotation>
                                            <xs:appinfo>SDScomChem</xs:appinfo>
                                            <xs:documentation> Product Subcategory: The type of paint or varnish </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element minOccurs="0" name="VocLimitForSubcategory" type="eSDScom:NumericValueWithUnitAndQualifier">
                                        <xs:annotation>
                                            <xs:appinfo>SDScomChem</xs:appinfo>
                                            <xs:documentation>Maximum VOC content limit for the mixture: The maximum concentration of VOC in the actual product. To be given in grams per litre. </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element minOccurs="0" name="MaxVocConcInMixture" type="eSDScom:NumericValueWithUnitAndQualifier">
                                        <xs:annotation>
                                            <xs:appinfo>SDScomChem</xs:appinfo>
                                            <xs:documentation>Actual max. concentration in the mixture: The concentration of VOC in the mixture. To be given in grams per litre. </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="DetergentLabelling" type="eSDScom:Phrase">
                            <xs:annotation>
                                <xs:documentation> Detergent labelling: Labelling requirements according to detergents regulation (648/2004/EC with later amanedments). Includes dosing recommendations. Concentration groups: _lt 5 weight%, 5 - _lt 15 vekt%, 15 - _lt 30 weight%, og _ge 30 weight%. </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="AerosolLabelling" type="eSDScom:Phrase">
                            <xs:annotation>
                                <xs:documentation> Aerosol labelling: Labelling of aerosol dispensers in accordance with EU directive (75/324/EEC with later amendments) </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="OtherEuLabellingRequirements" type="eSDScom:Phrase">
                            <xs:annotation>
                                <xs:documentation> Other labelling requirenments in accordance with EU legislation: Labelling requirements imposed to the chemical in accordance with EU legislation not stated above. </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ComponentUS">
        <xs:complexContent>
            <xs:extension base="eSDScom:Component">
                <xs:sequence>
                    <xs:element minOccurs="0" name="Classification" type="eSDScom:ClassificationUS"> </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="OnLabel" type="boolean">
                        <xs:annotation>
                            <xs:appinfo>SDScomChem</xs:appinfo>
                            <xs:documentation> Name of the component to be displayed on the hazard label: The value "True" for this means that the name of the actual component shall be printed on the hazard label as well as in section 2 of the SDS under the sub-heading 'Dangerous substances'�. The default value of this element will be 'false'�. </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="UsTransportInformation">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element maxOccurs="1" minOccurs="0" name="IsDangerReleasingSubstance" type="boolean">
                                    <xs:annotation>
                                        <xs:documentation>according to DOT</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="RegulatoryInformation">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element minOccurs="0" name="Federal">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element minOccurs="0" name="OshaStandards" type="eSDScom:Phrase">
                                                <xs:annotation>
                                                    <xs:documentation>Chemical hazards and toxic substances are addressed in specific OSHA standards for general industry, shipyard employment, marine terminals, longshoring and construction. This highlights OSHA standards and documents related to chemical hazards and toxic substances, i.e. provides links to relevant literature.
https://www.osha.gov/SLTC/hazardoustoxicsubstances/requirements.html</xs:documentation>
                                                </xs:annotation>
                                            </xs:element>
                                            <xs:element minOccurs="0" name="Sara313Threshold" type="eSDScom:NumericValue">
                                                <xs:annotation>
                                                    <xs:documentation>Superfund Amendments and Reauthorization Act</xs:documentation>
                                                </xs:annotation>
                                            </xs:element>
                                            <xs:element minOccurs="0" name="Sara302HazardousRQ" type="eSDScom:NumericValue"/>
                                            <xs:element minOccurs="0" name="Sara302ExtremelyHazardousRQ" type="eSDScom:NumericValue"/>
                                            <xs:element minOccurs="0" name="Sara302ExtremelyHazardousTPQ" type="eSDScom:NumericValue"/>
                                            <xs:element minOccurs="0" name="CwaToxicPollutant" type="boolean">
                                                <xs:annotation>
                                                    <xs:documentation>Clean Water Act</xs:documentation>
                                                </xs:annotation>
                                            </xs:element>
                                            <xs:element minOccurs="0" name="CwaPriorityPollutant" type="boolean"/>
                                            <xs:element maxOccurs="unbounded" minOccurs="0" name="DrugPrecursors" type="eSDScom:Phrase">
                                                <xs:annotation>
                                                    <xs:documentation>Controlled Substances Act (CSA)
Chemical Diversion and Trafficking Act</xs:documentation>
                                                </xs:annotation>
                                            </xs:element>
                                            <xs:element maxOccurs="unbounded" minOccurs="0" name="WeaponsPrecursors" type="eSDScom:Phrase">
                                                <xs:annotation>
                                                    <xs:documentation>Chemical Weapon Convention (CWC)</xs:documentation>
                                                </xs:annotation>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                                <xs:element minOccurs="0" name="State">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element minOccurs="0" name="California">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element minOccurs="0" name="Proposition65">
                                                            <xs:complexType>
                                                                <xs:choice>
                                                                    <xs:element name="Candidate" type="boolean">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Allows to indicate substances under discussion for inclusion</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                    <xs:sequence>
                                                                        <xs:element name="Listed" type="boolean"/>
                                                                        <xs:element minOccurs="0" name="ToxType" type="string"/>
                                                                        <xs:element minOccurs="0" name="Listing" type="string"/>
                                                                    </xs:sequence>
                                                                </xs:choice>
                                                            </xs:complexType>
                                                        </xs:element>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                            <xs:element minOccurs="0" name="RhodeIsland">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element minOccurs="0" name="HazardousSubstancesList">
                                                            <xs:annotation>
                                                                <xs:documentation>see http://www.dlt.ri.gov/occusafe/pdfs/HazardousCAS.pdf</xs:documentation>
                                                            </xs:annotation>
                                                            <xs:complexType>
                                                                <xs:sequence>
                                                                    <xs:element name="Listed" type="boolean"/>
                                                                    <xs:element minOccurs="0" name="SourceAcgih" type="boolean">
                                                                        <xs:annotation>
                                                                            <xs:documentation>In the list, denoted by the letter T.</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                    <xs:element minOccurs="0" name="SourceNfpa49" type="boolean">
                                                                        <xs:annotation>
                                                                            <xs:documentation>In the list, denoted by the letter F.</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                    <xs:element minOccurs="0" name="SourceIarc" type="boolean">
                                                                        <xs:annotation>
                                                                            <xs:documentation>In the list, denoted by the letter C.</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                </xs:sequence>
                                                            </xs:complexType>
                                                        </xs:element>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="DisposalConsiderationsUS">
        <xs:sequence>
            <xs:element minOccurs="0" name="Federal">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="RcraWasteCode" type="string"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="HazardousWasteCharacteristics" type="eSDScom:Phrase"> </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="RcraBasisForListing" type="eSDScom:Phrase"> </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="State">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="California">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element maxOccurs="unbounded" minOccurs="0" name="HazardousWasteCode" type="string"/>
                                    <xs:element maxOccurs="unbounded" minOccurs="0" name="HazardousWasteStatus" type="eSDScom:Phrase"> </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
