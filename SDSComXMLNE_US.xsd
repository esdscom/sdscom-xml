                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element minOccurs="0" name="Pennsylvania">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element maxOccurs="unbounded" minOccurs="0" name="RightToKnow" type="SDScom:Phrase"> </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="OtherInformationUS">
        <xs:sequence>
            <xs:element minOccurs="0" name="NfpaHazardRating">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Health">
                            <xs:simpleType>
                                <xs:restriction base="int">
                                    <xs:minInclusive value="0"/>
