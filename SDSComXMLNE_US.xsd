                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="EpaPesticideRegNo" type="string"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="EpaPesticideStatement" type="SDScom:Phrase"> </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="EpaPesticideLabel" type="SDScom:Phrase"> </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="DrugPrecursors" type="SDScom:Phrase"> </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="WeaponsPrecursors" type="SDScom:Phrase"> </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="State">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="0" name="Massachusetts">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element maxOccurs="unbounded" minOccurs="0" name="RightToKnow" type="SDScom:Phrase"> </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element minOccurs="0" name="NewJersey">
                            <xs:complexType>
